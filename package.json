{
  "name": "yoink-my-logs",
  "version": "0.4.0",
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./browser": {
      "import": "./browser.js",
      "types": "./browser.d.ts"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "node --test --test-concurrency=1 test/**/*.test.js",
    "test:server": "node --test --test-concurrency=1 test/server/*.test.js",
    "test:client": "node --test --test-concurrency=1 test/client/*.test.js",
    "demo": "node bin/demo.js"
  },
  "files": [
    "index.js",
    "index.cjs",
    "index.d.ts",
    "browser.js",
    "browser.d.ts",
    "server.js",
    "public/",
    "bin/"
  ],
  "keywords": [
    "logs",
    "debug",
    "sse",
    "log-viewer"
  ],
  "author": "Alexandros Tzotzolas",
  "description": "Local log viewer without dependencies. Use `yoink()` to write logs and `yoink` to start the browser UI.",
  "bin": {
    "yoink": "./bin/ui.js",
    "yoink-demo": "./bin/demo.js"
  },
  "homepage": "https://github.com/aletzo/yoink-my-logs",
  "repository": {
    "type": "git",
    "url": "https://github.com/aletzo/yoink-my-logs.git"
  },
  "bugs": {
    "url": "https://github.com/aletzo/yoink-my-logs/issues"
  },
  "license": "MIT"
}
